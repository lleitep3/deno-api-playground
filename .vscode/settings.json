{
  "deno.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always",
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[tiltfile]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "golang.go"
  }
}